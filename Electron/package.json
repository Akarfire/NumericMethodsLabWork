{
    "name": "decision-support-system",

    "version": "1.0.0",

    "description": "",

    "main": "main.js",

    "scripts": {
        "start": "electron .",
        "build": "electron-builder"
    },

    "keywords": [],

    "author": "",

    "license": "ISC",

    "type": "commonjs",

    "devDependencies": {
        "electron": "^39.2.4",
        "electron-builder": "^26.0.12"
    },

    "build": {


        "electronLanguages": ["en-US"],

        "appId": "com.fiit.numericlab",

        "productName": "DecisionSupportSystem",

        "icon": "assets/icon.ico",

        "files": [
            "**/*",
            "!python/**"
        ],

        "directories": {
            "buildResources": "assets"
        },

        "forceCodeSigning": false,

        "win": {
            "target": ["portable"]
        },

        "extraResources": [
            {
                "from": "python/",
                "to": "python/"
            },
            {
                "from": "../Source/",
                "to": "python-src/"
            },
            {
                "from": "../UI/",
                "to": "ui/"
            }
        ],

        "extraMetadata": {
            "electronDebug": false
        }
    }
}
